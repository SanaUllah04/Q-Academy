        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.4.23",
        "eslint": "^8",
        "eslint-config-next": "^15",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.19",
        "typescript": "^5",
        "wrangler": "^3"
    }
}
